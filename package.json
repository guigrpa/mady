{
  "name": "mady",
  "version": "3.3.0",
  "description": "Easy-to-use tool to manage and translate ICU MessageFormat messages",
  "bin": {
    "mady": "lib/mady.js"
  },
  "main": "lib/translate.js",
  "engines": {
    "node": ">=4"
  },
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "keywords": [
    "i18n",
    "MessageFormat",
    "translation",
    "locales",
    "translator",
    "React Intl"
  ],
  "homepage": "https://github.com/guigrpa/mady#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/mady/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/mady.git"
  },
  "_devSetup": {
    "install": "yarn && brew install watchman",
    "term1": "yarn compileWatch",
    "term2": "yarn compile:relayWatch",
    "term3": "yarn buildClientWatch",
    "term4": "yarn start",
    "term5": "yarn jest"
  },
  "scripts": {
    "start": "node lib/server/startup --dir src/locales",
    "startAlt": "node lib/server/startup --dir tools/locales",
    "startProd": "node lib/mady --dir src/locales",
    "compile": "yarn compile:clean && yarn compile:copy && yarn compile:main && yarn compile:copy2",
    "compileWatch": "yarn compile:clean && yarn compile:copy && yarn compile:mainWatch",
    "compile:clean": "rm -rf ./lib && mkdir lib",
    "compile:copy": "cp src/api.js.flow lib/translate.js.flow",
    "compile:copy2": "cp src/client/components/aaApp.sass lib/client/components/",
    "compile:main": "babel src -d lib --ignore \"**/__mocks__/**\",\"**/__tests__/**\"",
    "compile:mainWatch": "yarn compile:main --watch",
    "compile:relay": "yarn compile:schema && relay-compiler --src ./src --schema ./src/common/gqlSchema.graphql",
    "compile:relayWatch": "yarn compile:relay --watch",
    "compile:schema": "node lib/server/graphql/updateSchema",
    "build": "yarn lint && yarn flow && yarn compile && yarn compile:relay && yarn buildClient && yarn buildSsr && yarn test && yarn docs && yarn xxl",
    "buildSsr": "yarn build:copyPublic && yarn build:webpack -p --mode production --env.NODE_ENV=production --env.SERVER_SIDE_RENDERING=true",
    "buildSsrWatch": "yarn build:copyPublic && yarn build:webpack -w --mode development --env.SERVER_SIDE_RENDERING=true",
    "buildClient": "yarn build:copyPublic && yarn build:webpack -p --mode production --env.NODE_ENV=production",
    "buildClientWatch": "yarn build:copyPublic && yarn build:webpack -w --mode development",
    "buildAnalysis": "yarn build:copyPublic && yarn build:webpack --env.ANALYZE_BUNDLE=true",
    "buildAnalysisProd": "yarn build:copyPublic && yarn build:webpack -p --mode production --env.NODE_ENV=production --env.ANALYZE_BUNDLE=true",
    "build:copyPublic": "cp -r src/public lib",
    "build:webpack": "webpack --config ./src/webpackConfig.js --color --progress --display-chunks",
    "travis": "yarn compile && yarn test",
    "docs": "extract-docs --template docs/templates/README.md --output README.md",
    "lint": "eslint src",
    "flow": "flow check || exit 0",
    "xxl": "xxl",
    "test": "yarn testCovPrepare && yarn testDev && yarn testProd && yarn testCovReport",
    "testCovFast": "yarn testCovPrepare && yarn testDev && yarn testCovReport",
    "jest": "jest --watch --coverage",
    "jest-html": "jest-html --snapshot-patterns \"src/**/*.snap\"",
    "jestDebug": "node --debug-brk --inspect node_modules/.bin/jest -i",
    "testCovPrepare": "rm -rf ./coverage .nyc_output .nyc_tmp && mkdir .nyc_tmp",
    "testDev": "NODE_ENV=development jest --coverage --forceExit && node tools/processCoverageFile dev",
    "testProd": "NODE_ENV=production jest --coverage --forceExit && node tools/processCoverageFile prod",
    "testMin": "TEST_MINIFIED_LIB=1 jest --coverage --forceExit && node tools/processCoverageFile min",
    "testCovReport": "cp .nyc_tmp/* .nyc_output/ && nyc report --reporter=html --reporter=lcov --reporter=text",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.js\""
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "buffer": "^5.2.1",
    "chokidar": "^2.0.4",
    "commander": "^2.19.0",
    "compression": "^1.7.3",
    "cookie-parser": "1.4.3",
    "diveSync": "0.3.0",
    "express": "^4.16.4",
    "express-graphql": "^0.6.12",
    "fs-extra": "0.30.0",
    "google-translate-token": "1.0.0",
    "graphql": "0.10.5",
    "graphql-relay": "0.5.2",
    "graphql-subscriptions": "0.4.4",
    "isomorphic-fetch": "2.2.1",
    "lodash": "^4.17.11",
    "messageformat": "1.0.2",
    "opn": "^5.4.0",
    "querystring": "^0.2.0",
    "slash": "1.0.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "storyboard": "^3.1.4",
    "storyboard-listener-console": "^3.1.4",
    "storyboard-listener-ws-server": "^3.1.4",
    "terser": "^3.10.11",
    "timm": "^1.6.1",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-0": "^6.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-relay": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "coveralls": "2.13.1",
    "css-loader": "^0.28.10",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-docs": "^1.6.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "flow-bin": "0.50.0",
    "font-awesome": "4.7.0",
    "giu": "0.14.1",
    "jest": "^23.6.0",
    "jest-html": "^1.5.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.4",
    "null-loader": "0.1.1",
    "nyc": "^13.1.0",
    "oao": "^1.5.1",
    "prettier": "^1.14.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-intl": "^2.7.2",
    "react-relay": "^1.2.0",
    "react-test-renderer": "^16.6.3",
    "relay-compiler": "^1.2.0",
    "relay-runtime": "^1.2.0",
    "sass-loader": "^6.0.7",
    "storyboard-listener-browser-extension": "^3.1.4",
    "storyboard-listener-ws-client": "^3.1.4",
    "style-loader": "^0.20.3",
    "tiny-cookie": "0.5.5",
    "tinycolor2": "1.4.1",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "xxl": "^1.2.0"
  },
  "jest": {
    "testRegex": "src/.*__tests__/.*\\.test\\.js$",
    "moduleNameMapper": {
      "^typeface-.*": "<rootDir>/test/emptyObject.js",
      "\\.(css|less|sass)$": "<rootDir>/test/emptyObject.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/test/emptyString.js"
    },
    "coverageDirectory": ".nyc_output",
    "coverageReporters": [
      "json",
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-html"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/__mocks__/**",
      "!**/__generated__/**",
      "!src/locales/**",
      "!src/localesBackup/**",
      "!src/exampleLocales.js",
      "!src/mady.js",
      "!src/server/importData.js",
      "!src/client/components/zzReactIntlExample.js",
      "!src/client/components/fetchLangBundle.js"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js"
  }
}
